<div class="row text-center white-text" id="add-monster-banner">
<h1><%= @header %> on Monsters.com</h1>
</div>

<div class="row text-center" id="add-backgound-light">
<ul class="no-bullets" id="monster-list">
  <% @monsters.each do |monster| %>
    <li><h3><%= link_to monster.name, monster %> </h3></li>
  <% end %>
</ul>
<%= link_to 'Add new monster', new_monster_path, class: "button" %>
</div>

<div id="app"></div>
